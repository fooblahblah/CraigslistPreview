{
    "name": "Craigslist Preview",
    "version": "1.0.22",
    "manifest_version" : 2,
    "description": "Extension to prefetch Craigslist ad images",
    "icons": { "48": "icon48.png",
               "128": "icon128.png" },
    "permissions": [
        "*://*.craigslist.org/*",
        "*://*.craigslist.ca/*",
        "*://*.craigslist.at/*",
        "*://*.craigslist.cz/*",
        "*://*.craigslist.fi/*",
        "*://*.craigslist.de/*",
        "*://*.craigslist.gr/*",
        "*://*.craigslist.it/*",
        "*://*.craigslist.pl/*",
        "*://*.craigslist.pt/*",
        "*://*.craigslist.es/*",
        "*://*.craigslist.se/*",
        "*://*.craigslist.ch/*",
        "*://*.craigslist.com/*",
        "*://*.craigslist.co/*",
        "*://*.craigslist.hk/*",
        "*://*.craigslist.jp/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.craigslist.org/*",
                "*://*.craigslist.ca/*",
                "*://*.craigslist.at/*",
                "*://*.craigslist.cz/*",
                "*://*.craigslist.fi/*",
                "*://*.craigslist.de/*",
                "*://*.craigslist.gr/*",
                "*://*.craigslist.it/*",
                "*://*.craigslist.pl/*",
                "*://*.craigslist.pt/*",
                "*://*.craigslist.es/*",
                "*://*.craigslist.se/*",
                "*://*.craigslist.ch/*",
                "*://*.craigslist.com/*",
                "*://*.craigslist.co/*",
                "*://*.craigslist.hk/*",
                "*://*.craigslist.jp/*"
            ],
            "js": [
                "jquery-1.3.2.min.js",
                "jquery-ui-1.7.2.custom.min.js",
                "jquery.ba-dotimeout.js",
                "cl.js"],
            "css": ["cl.css"],
            "run_at": "document_start"
        }
    ]
}
